<p id="notice"><%= notice %></p>

<h1>Rcables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rcables.each do |rcable| %>
      <tr>
        <td><%= rcable.name %></td>
        <td><%= rcable.user_id %></td>
        <td><%= link_to 'Show', rcable %></td>
        <td><%= link_to 'Edit', edit_rcable_path(rcable) %></td>
        <td><%= link_to 'Destroy', rcable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rcable', new_rcable_path %>
